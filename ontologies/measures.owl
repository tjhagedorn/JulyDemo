    <owl:Class rdf:about="http://raw.githubusecontent.com/decision/UMassCenterforEDesign/master/drone_aircraft#measurement_capability">
        <rdfs:subClassOf rdf:resource="https://raw.githubusercontent.com/UMassCenterforeDesign/ICAM/master/BEM2.0.owl#capability"/>
        <ExtendedRelationOntology:definition>a measurement capability is a capability that inheres in a continuant having the ability to numerically characterize some other entity up to some level of quality</ExtendedRelationOntology:definition>
        <rdfs:label>measurement capability</rdfs:label>
    </owl:Class>
    


    <!-- http://raw.githubusecontent.com/decision/UMassCenterforEDesign/master/drone_aircraft#measurement_precision_capability -->

    <owl:Class rdf:about="http://raw.githubusecontent.com/decision/UMassCenterforEDesign/master/drone_aircraft#measurement_precision_capability">
        <rdfs:subClassOf rdf:resource="http://raw.githubusecontent.com/decision/UMassCenterforEDesign/master/drone_aircraft#measurement_quality_capability"/>
    </owl:Class>
    


    <!-- http://raw.githubusecontent.com/decision/UMassCenterforEDesign/master/drone_aircraft#measurement_quality_capability -->

    <owl:Class rdf:about="http://raw.githubusecontent.com/decision/UMassCenterforEDesign/master/drone_aircraft#measurement_quality_capability">
        <rdfs:subClassOf rdf:resource="http://raw.githubusecontent.com/decision/UMassCenterforEDesign/master/drone_aircraft#measurement_capability"/>
        <ExtendedRelationOntology:definition>a measurement quality capability is a measurement capability that when realized allows some entity performing a measurement to obtain measurements having certain desirable features</ExtendedRelationOntology:definition>
        <rdfs:label>measurement quality capability</rdfs:label>
    </owl:Class>
    


    <!-- http://raw.githubusecontent.com/decision/UMassCenterforEDesign/master/drone_aircraft#measurement_resolution_capability -->

    <owl:Class rdf:about="http://raw.githubusecontent.com/decision/UMassCenterforEDesign/master/drone_aircraft#measurement_resolution_capability">
        <rdfs:subClassOf rdf:resource="http://raw.githubusecontent.com/decision/UMassCenterforEDesign/master/drone_aircraft#measurement_quality_capability"/>
        <ExtendedRelationOntology:definition>a measurement resolution capability is a measurement capability that when realized allows the bearer to measure some phenomenon at some minimum scale</ExtendedRelationOntology:definition>
    </owl:Class>
    


    <!-- http://raw.githubusecontent.com/decision/UMassCenterforEDesign/master/drone_aircraft#sensitivity_capability -->

    <owl:Class rdf:about="http://raw.githubusecontent.com/decision/UMassCenterforEDesign/master/drone_aircraft#sensitivity_capability">
        <rdfs:subClassOf rdf:resource="http://raw.githubusecontent.com/decision/UMassCenterforEDesign/master/drone_aircraft#detection_capability"/>
        <ExtendedRelationOntology:definition>a sensitivity classification capability is a detection capability that when realized allows its bearer to identify &quot;true positives&quot; in which the detection target is present and the bearer correctly identifies it as such</ExtendedRelationOntology:definition>
    </owl:Class>
    


    <!-- http://raw.githubusecontent.com/decision/UMassCenterforEDesign/master/drone_aircraft#specificity_capability -->

    <owl:Class rdf:about="http://raw.githubusecontent.com/decision/UMassCenterforEDesign/master/drone_aircraft#specificity_capability">
        <rdfs:subClassOf rdf:resource="http://raw.githubusecontent.com/decision/UMassCenterforEDesign/master/drone_aircraft#detection_capability"/>
        <ExtendedRelationOntology:definition>a sensitivity classification capability is a detection capability that when realized allows its bearer to identify &quot;true negatives&quot; in which the detection target is not present and the bearer correctly identifies it as such</ExtendedRelationOntology:definition>
    </owl:Class>
    


    <!-- http://raw.githubusecontent.com/decision/UMassCenterforEDesign/master/drone_aircraft#spectrum_measurement_capability -->

