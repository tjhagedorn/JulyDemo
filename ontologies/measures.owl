    <!-- http://raw.githubusecontent.com/decision/UMassCenterforEDesign/master/drone_aircraft#detection_capability -->

    <owl:Class rdf:about="http://raw.githubusecontent.com/decision/UMassCenterforEDesign/master/drone_aircraft#detection_capability">
        <rdfs:subClassOf rdf:resource="https://raw.githubusercontent.com/UMassCenterforeDesign/ICAM/master/BEM2.0.owl#capability"/>
        <ExtendedRelationOntology:definition>a detection capability is a capability that when realized allows the bearer to successfully distinguish between presence and absence of some entity  or distinguish between said target entity and some other entity during a measurement process</ExtendedRelationOntology:definition>
        <rdfs:label>detection capability</rdfs:label>
    </owl:Class>
    


